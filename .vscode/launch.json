  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug: Root",
      "type": "pwa-chrome",
      "request": "launch",
      "url": "http://localhost:3000/",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Debug: Card",
      "type": "pwa-chrome",
      "request": "launch",
      "url": "http://localhost:3000/card",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Debug: Login",
      "type": "pwa-chrome",
      "request": "launch",
      "url": "http://localhost:3000/login",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Debug: Recover Password",
      "type": "pwa-chrome",
      "request": "launch",
      "url": "http://localhost:3000/login/recover-password/3",
      "webRoot": "${workspaceFolder}"
    }
  ]
}
